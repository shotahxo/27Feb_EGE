from turtle import *

screensize(1000,1000)
pensize(3)
tracer(50)
	
n=10
down()
for i in range(777):
	forward(25*n)
	left(90)
	forward(34*n)
	left(90)
up()
forward(12*n)
left(90)
forward(17*n)
right(90)
down()
for i in range(1996):
	forward(25*n)
	left(90)
	forward(17*n)
	left(90)

up()
for x in range(-100,100):
	for y in range(-100,100):
		goto(x*n,y*n)
		dot(3,'red')